{% extends 'base.html.twig' %}

{% block title %}Objects{% endblock %}

{% block body %}
    <h1>Mes objets</h1>
    <a href="/new"><button class="btn btn-primary btn-sm">Ajouter un objet</button></a>
{% if objects is defined %}
    <table class="table">
        <tbody>
    {% for object in objects %}
            <tr>
                <th>Id</th>
                <td>{{ object.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ object.Type }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ object.Name }}</td>
            </tr>
            <tr>
                <!-- ici placer le donner -->
            </tr>
    {% endfor %}
        </tbody>
    </table>
{% else %}
    <p>Vous ne posseder pas d'objets</p>
{% endif %}
    <a href="{{ path('objects_index') }}">back to list</a>
{% endblock %}
